<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace 對應持久層的介面 -->
<mapper namespace="com.ampletec.gambling.report.mapper.ScheduleRecordMapper" >


    <insert id="insert">
        INSERT INTO
            ScheduleRecord (scheduleid,status,start,end)
        VALUES
            (#{scheduleid}, #{status}, #{start}, #{end})
    </insert>

</mapper>